<?xml version="1.0" encoding="UTF-8"?>
<botdetect xmlns="https://captcha.com/schema/net"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://captcha.com/schema/net
            https://captcha.com/schema/net/botdetect-4.4.0.xsd">

    <!-- For more information on how to configure CAPTCHA configuration options,
		visit https://captcha.com/doc/aspnet/net-captcha-options.html#simple-api
		-->
	<captchaStyles>
        <captchaStyle>
            <name>angularBasicCaptcha</name>
            <userInputID>captchaCode</userInputID>
            <codeLength>3</codeLength>
            <helpLinkMode>Image</helpLinkMode>
        </captchaStyle>

        <captchaStyle>
            <name>angularFormCaptcha</name>
            <userInputID>captchaCode</userInputID>
            <codeLength>2</codeLength>
            <helpLinkMode>Image</helpLinkMode>
        </captchaStyle>
    </captchaStyles>
    
</botdetect>